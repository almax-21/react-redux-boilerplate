{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "react-redux-boilerplate",
  "browserslist": "> 0.25%, not dead",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "dev": "cross-env NODE_ENV=development npx webpack --mode development",
    "build": "cross-env NODE_ENV=production npx webpack --mode production",
    "start": "node server.js",
    "start:dev": "cross-env NODE_ENV=development webpack serve --mode development --open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "npx commitlint --edit $1"
    }
  },
  "repository": {
		"type": "git",
		"url": "git+https://github.com/almax-21/react-redux-boilerplate.git"
	},
	"author": "Maksim Almaev",
  "license": "ISC",
